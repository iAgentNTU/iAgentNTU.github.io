	Keywords = {Multiagent, hybrid system,wheeld mobile manipulator,The Treasure Hunter},
	Month = {December},
	Owner = {yjhsu},
	Timestamp = {2006.03.28},
	Title = {A Nonlinear Multiagent System Design with A Hybrid Approach},
	Year = {2005}}

